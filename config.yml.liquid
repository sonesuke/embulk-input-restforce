in:
  type: restforce
  user_name: {{ env.SALESFORCE_USER_NAME }}
  password: {{ env.SALESFORCE_PASSWORD }}
  security_token: {{ env.SALESFORCE_SECURITY_TOKEN }}
  client_id : {{ env.SALESFORCE_CLIENT_ID }}
  client_secret : {{ env.SALESFORCE_CLIENT_SECRET }}
  api : 54.0
  sobject: Lead
  skip_columns:
    - {pattern: Name, ignore: [Lead]}
  hashed_columns:
    - {pattern: LastName, ignore: [Lead]}
    - {pattern: FirstName}
out:
  type: stdout